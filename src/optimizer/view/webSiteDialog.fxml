<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/dark.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="optimizer.view.WebSiteDialogController">
   <children>
      <Button fx:id="webButton" layoutX="38.0" layoutY="138.0" mnemonicParsing="false" onAction="#pingIp" prefHeight="100.0" prefWidth="200.0" text="Check Website">
         <font>
            <Font name="Catamaran Bold" size="20.0" />
         </font>
      </Button>
      <TextField fx:id="webTextField" layoutX="253.0" layoutY="176.0" prefHeight="26.0" prefWidth="289.0" promptText="google.com">
         <font>
            <Font name="Catamaran SemiBold" size="15.0" />
         </font></TextField>
      <Label fx:id="webLabel1" layoutX="295.0" layoutY="131.0" text="Enter the website address here:">
         <font>
            <Font name="Catamaran SemiBold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="webLabel2" layoutX="254.0" layoutY="249.0" prefHeight="27.0" prefWidth="339.0">
         <font>
            <Font name="Catamaran SemiBold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="gotItWebSiteLayer" layoutX="297.0" layoutY="354.0" mnemonicParsing="false" onAction="#closeWindow" text="Close" />
   </children>
</AnchorPane>
